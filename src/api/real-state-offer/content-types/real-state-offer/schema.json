{
  "kind": "collectionType",
  "collectionName": "real_state_offers",
  "info": {
    "singularName": "real-state-offer",
    "pluralName": "real-state-offers",
    "displayName": "Real State Offer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "country": {
      "type": "customField",
      "customField": "plugin::country-select.country"
    },
    "offerType": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Venta:venta",
        "Arriendo:arriendo"
      ]
    },
    "propertyUse": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Data Center:data-center",
        "Log√≠stica:logistica",
        "Manufactura:manufactura",
        "Servicio:servicio"
      ]
    },
    "propertyType": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Bodega:bodega",
        "Lote:lote",
        "Oficina:oficina"
      ]
    },
    "area": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "region": {
      "type": "string"
    },
    "imgGallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "platinum": {
      "type": "boolean",
      "default": false
    },
    "certifications": {
      "type": "blocks"
    },
    "propertyStatus": {
      "type": "enumeration",
      "enum": [
        "Nuevo",
        "Usado"
      ]
    },
    "ctaButton": {
      "type": "component",
      "component": "components.button",
      "repeatable": false
    },
    "affiliateCompany": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::affiliate.affiliate",
      "inversedBy": "realStateOffers"
    },
    "users": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "realStateOffers"
    }
  }
}
