{
  "kind": "collectionType",
  "collectionName": "press_rooms",
  "info": {
    "singularName": "press-room",
    "pluralName": "press-rooms",
    "displayName": "Press Room",
    "description": "Press room content with publication controls"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "isPublished": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "scheduledPublishDate": {
      "type": "datetime",
      "required": false
    },
    "extract": {
      "type": "richtext"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "news",
        "podcast",
        "newsletter",
        "blog"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::press-room-category.press-room-category",
      "inversedBy": "pressRooms"
    },
    "externalLink": {
      "type": "string"
    },
    "downloadDocument": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    }
  }
}
